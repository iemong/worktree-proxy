{
  "name": "@iemong/kirikae",
  "version": "1.0.7",
  "module": "proxy.ts",
  "type": "module",
  "bin": {
    "kirikae": "./proxy.ts"
  },
  "exports": {
    ".": "./proxy.ts"
  },
  "scripts": {
    "start": "bun run proxy.ts",
    "dev": "bun run --watch proxy.ts",
    "lint": "biome lint .",
    "format": "biome format .",
    "check": "biome check .",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "changeset": "changeset",
    "changeset:version": "changeset version && bun run sync-version",
    "changeset:publish": "changeset publish",
    "changeset:status": "changeset status",
    "sync-version": "bun run scripts/sync-version.ts",
    "publish:jsr": "bunx jsr publish"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@changesets/cli": "^2.29.8",
    "@types/bun": "latest",
    "ultracite": "7.0.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "hono": "^4.11.3"
  }
}
